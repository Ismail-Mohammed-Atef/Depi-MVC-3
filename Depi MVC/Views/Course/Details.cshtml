@model Course
@{
}
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Course</h5>
        <p class="card-text">
            <tr>
                <th scope="row">@Model.Id</th>
                <td>@Model.Name</td>
                <td>@Model.MaxDegree</td>
                <td>@Model.MinDegree</td>
            </tr>
        </p>
        <a href="/Course/Index" class="btn btn-primary">Back</a>
    </div>
</div>